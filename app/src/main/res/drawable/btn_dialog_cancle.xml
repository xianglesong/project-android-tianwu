<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <!--填充颜色为透明-->
            <solid android:color="@color/btn_cancel_solid_normal" />
            <!--边框的颜色和大小-->
            <stroke android:width="1dp" android:color="@color/btn_cancel_stroke" />
            <corners android:radius="8dp" />
            <!--边框弧形的半径-->
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <!--填充颜色为透明-->
            <solid android:color="@color/btn_cancel_solid_pressed" />
            <!--边框的颜色和大小-->
            <stroke android:width="1dp" android:color="@color/btn_cancel_stroke" />
            <corners android:radius="8dp" />
            <!--边框弧形的半径-->
        </shape>
    </item>
</selector>